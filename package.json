{
  "name": "capacitor-contact-manager",
  "version": "0.2.4",
  "description": "A plugin for managing native phone contacts",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && typedoc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "readme": "Readme.md",
  "author": "Chris Woolum",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "rimraf": "^3.0.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorContactManager.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cwoolum/capacitor-contacts"
  },
  "bugs": {
    "url": "https://github.com/cwoolum/capacitor-contacts/issues"
  }
